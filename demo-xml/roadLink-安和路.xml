<?xml version="1.0" encoding="UTF-8"?>
<ro:RoadLink xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gmd="http://www.isotc211.org/2005/gmd"
    xmlns:ro="http://www.motc.gov.tw/RoadNetwork"
    xmlns:net="http://www.motc.gov.tw/TransportNetwork" xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gss="http://www.isotc211.org/2005/gss"
    xmlns:gsr="http://www.isotc211.org/2005/gsr" xmlns:gts="http://www.isotc211.org/2005/gts"
    xmlns:base="http://www.motc.gov.tw/BaseTypes" xmlns="http://www.motc.gov.tw/RoadEvent"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.motc.gov.tw/RoadEvent ../XSD/motcRoadEvent.xsd 
                        http://www.motc.gov.tw/RoadNetwork ../XSD/motcRoadNetwork.xsd"
    gml:id="link" >    
    <!--路名,非必要-->
    <net:referedProperty>
        <ro:RoadIdentity gml:id="link1-id"> 
            <ro:roadName>安和路</ro:roadName>       
        </ro:RoadIdentity>
    </net:referedProperty>
    <!-- 共線名稱,非必要 -->
    <net:referedProperty>
        <ro:RoadIdentity gml:id="link1-id-alias"> 
            <ro:roadName>縣125</ro:roadName>
            <ro:roadCode>125</ro:roadCode>
            <ro:roadClass>2U</ro:roadClass>            
        </ro:RoadIdentity>
    </net:referedProperty>
    <!-- 線段結束點,非必要 -->
    <net:endNode>
        <ro:RoadNode gml:id="endNode">
            <!--節點id, 必要-->
            <ro:roadNodeID>S6EX9LKJ1</ro:roadNodeID>        
        </ro:RoadNode>
    </net:endNode>
    <!-- 線段起始點,非必要 -->
    <net:startNode>
        <ro:RoadNode gml:id="startNode">
            <!--節點id, 必要-->
            <ro:roadNodeID>S6EX9LKJ1</ro:roadNodeID>
        </ro:RoadNode>
    </net:startNode>   
    <!--幾何資訊,非必要-->
    <net:geometry>
        <gml:LineString gml:id="line" srsName="EPSG:3826">
            <gml:posList>212011 2675517 211945 2675393</gml:posList>
        </gml:LineString>
    </net:geometry>
    <!--是否為無義路段(因數化或其他需求而產生的虛擬物件), 非必要-->
    <ro:trivial>false</ro:trivial>
    <!-- 道路結構,非必要 -->
    <ro:structureType>ground</ro:structureType>
    <!-- 線段編碼,必要 -->
    <ro:roadLinkID>B-4U-050526</ro:roadLinkID>
    <!-- 道路類型,非必要 -->
    <ro:formOfRoad>generalRoad</ro:formOfRoad>
    <!-- 橋樑或隧道名稱,非必要 -->
    <ro:bridgeTunnelName></ro:bridgeTunnelName>
</ro:RoadLink>
