    <net:location>
        <ro:RoadPointReference gml:id="ID039">    
            <net:element>
                <ro:RoadNode gml:id="S6F1ALKMV">
                    <!--是何線段的結束節點,非必要-->
                    <net:spokeEnd>
                        <ro:RoadLink gml:id="link1">
                            <!--路名,非必要-->
                            <net:referedProperty>
                                <ro:RoadIdentity gml:id="link1-id"> 
                                    <ro:roadName>西屯路三段</ro:roadName>       
                                </ro:RoadIdentity>
                            </net:referedProperty>            
                            <!-- 線段編碼,必要 -->
                            <ro:roadLinkID>B-6-244400</ro:roadLinkID>  
