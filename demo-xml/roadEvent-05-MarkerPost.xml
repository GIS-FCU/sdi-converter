<?xml version="1.0" encoding="UTF-8"?>
<RoadEvent xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gmd="http://www.isotc211.org/2005/gmd"
    xmlns:ro="http://www.motc.gov.tw/RoadNetwork"
    xmlns:net="http://www.motc.gov.tw/TransportNetwork" xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gss="http://www.isotc211.org/2005/gss"
    xmlns:gsr="http://www.isotc211.org/2005/gsr" xmlns:gts="http://www.isotc211.org/2005/gts"
    xmlns:base="http://www.motc.gov.tw/BaseTypes" xmlns="http://www.motc.gov.tw/RoadEvent"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.motc.gov.tw/RoadEvent ../XSD/motcRoadEvent.xsd
                        http://www.motc.gov.tw/RoadNetwork ../XSD/motcRoadNetwork.xsd
                        http://www.opengis.net/gml/3.2 ../XSD/gml/3.2.1.2/gml.xsd"
    gml:id="A_151026_0001">    
    <!-- 以里程椿通報 -->
    <net:location>
        <ro:RoadPointReference gml:id="ID039">
            <net:element xlink:type="simple">
                <ro:RoadMarkerPost gml:id="marker1">
                    <!--位於哪一條道路-->
                    <net:route>
                        <ro:Road gml:id="road1">
                            <!--路名，非必要-->                     
                            <net:referedProperty>
                                <ro:RoadIdentity gml:id="B2"> 
                                    <ro:roadName>縣125</ro:roadName>
                                    <ro:roadCode>125</ro:roadCode>
                                    <ro:roadClass>2U</ro:roadClass>
                                </ro:RoadIdentity>                
                            </net:referedProperty>
                            <net:link></net:link>
                            <!--道路編號，必要-->
                            <ro:roadID>road2</ro:roadID>                            
                        </ro:Road>
                    </net:route>
                    <!-- 自里程起始點開始起算的序號 -->
                    <net:sequenceNo>19</net:sequenceNo>
                    <!-- 自里程起始點開始起算的距離 -->
                    <net:mileage uom="m">9500</net:mileage>
                    <!-- 是否為虛擬椿 -->
                    <net:fictitious>true</net:fictitious>
                    <!--里程標識名稱-->
                    <ro:postTag>009K+500</ro:postTag>
                </ro:RoadMarkerPost>
            </net:element>
            <!--方向性，必要-->
            <net:direction>inOppositeDirection</net:direction>             
        </ro:RoadPointReference>
    </net:location>        
    <!-- 事件發生開始時間 -->
    <net:validFrom>2015-10-26T18:13:51.0</net:validFrom>
    <!-- 事件詮釋資料更新時間 -->    
    <net:updateTime>2015-10-26T18:13:51.0</net:updateTime>        
    <!-- 事件描述 -->
    <net:description>臺灣大道三段發生自小客車擦撞，無人員傷亡</net:description> 
    <net:eventID>201510260001</net:eventID>
    <!-- 處理狀態: 0=新增 -->
    <status>new</status>   
    <!-- 事件類別: 1=事故 -->
    <eventType>accident</eventType>    
</RoadEvent>
