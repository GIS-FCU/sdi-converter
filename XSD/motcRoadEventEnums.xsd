	<xs:simpleType name="BlockedLaneTypeValueType">
		<xs:annotation>
			<xs:documentation>阻斷車道數代碼</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="roevt:BlockedLaneTypeValueEnumerationType roevt:BlockedLaneTypeValueOtherType"/>
	</xs:simpleType>
	<xs:simpleType name="BlockedLaneTypeValueEnumerationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="shoulder">
				<xs:annotation>
					<xs:documentation>路肩</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="middle">
				<xs:annotation>
					<xs:documentation>中線</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="right">
				<xs:annotation>
					<xs:documentation>右線</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="left">
				<xs:annotation>
					<xs:documentation>左線</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="all">
				<xs:annotation>
					<xs:documentation>全線</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BlockedLaneTypeValueOtherType">
		<xs:restriction base="xs:string">
			<xs:pattern value="other: \w{2,}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BlockedLengthValueType">
		<xs:annotation>
			<xs:documentation>影響範圍代碼，以影響長度作為列舉來源</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="roevt:BlockedLengthValueEnumerationType roevt:BlockedLengthValueOtherType"/>
	</xs:simpleType>
	<xs:simpleType name="BlockedLengthValueEnumerationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="over10KM">
				<xs:annotation>
					<xs:documentation>大於等於10公里</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lessThan10KM">
				<xs:annotation>
					<xs:documentation>大於等於5公里，小於10公里</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lessThan5KM">
				<xs:annotation>
					<xs:documentation>大於等於1公里，小於5公里</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lessThan1KM">
				<xs:annotation>
					<xs:documentation>1km以下</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BlockedLengthValueOtherType">
		<xs:restriction base="xs:string">
			<xs:pattern value="other: \w{2,}"/>
		</xs:restriction>
	</xs:simpleType>
