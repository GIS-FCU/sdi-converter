				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4W">
				<xs:annotation>
					<xs:documentation>產業道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AL">
				<xs:annotation>
					<xs:documentation>巷弄。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HU">
				<xs:annotation>
					<xs:documentation>國道附屬道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HW">
				<xs:annotation>
					<xs:documentation>國道。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OR">
				<xs:annotation>
					<xs:documentation>路名不定。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OT">
				<xs:annotation>
					<xs:documentation>無路名道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RD">
				<xs:annotation>
					<xs:documentation>市區道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RE">
				<xs:annotation>
					<xs:documentation>市區快速道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RoadStructureTypeValue">
		<xs:annotation>
			<xs:documentation>道路結構種類代碼。</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="elevated">
				<xs:annotation>
					<xs:documentation>高架或橋樑</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ground">
				<xs:annotation>
					<xs:documentation>平面道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="tunnel">
				<xs:annotation>
					<xs:documentation>隧道。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="underGround">
				<xs:annotation>
					<xs:documentation>地下道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
