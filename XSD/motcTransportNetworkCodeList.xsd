	<xs:simpleType name="LinkDirectionValue">
		<xs:annotation>
			<xs:documentation>通行方向代碼，可為：單向通行(與數化方向同向或反向，預設為同向)、雙向通行。</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="oneway">
				<xs:annotation>
					<xs:documentation>單向通行(與數化方向同向)。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="twoway">
				<xs:annotation>
					<xs:documentation>雙向通行。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RoadClassValue">
		<xs:annotation>
			<xs:documentation>道路等級代碼。</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1E">
				<xs:annotation>
					<xs:documentation>省快速道路。</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1W">
